	month = jun,
	number = {4},
	pages = {1--9},
	title = {Evolutionary {{Dynamics}} of {{Microbial Communities}} in {{Bioelectrochemical Systems}}},
	url = {https://www.hilarispublisher.com/open-access/evolutionary-dynamics-of-microbial-communities-in-bioelectrochemical-systems.pdf},
	volume = {13},
