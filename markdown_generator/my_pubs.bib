	doi = {10.1093/bioinformatics/15.4.327},
	file = {/Users/lptolik/Zotero/storage/GPPDHWYX/Bioinformatics 1999 Sobolev.pdf;/Users/lptolik/Zotero/storage/W55YP6XV/Bioinformatics 1999 Sobolev.pdf},
	journal = {Bioinforma. Oxf. Engl.},
	langid = {english},
	month = apr,
	number = {4},
	pages = {327--332},
	pmid = {10320401},
	title = {Automated Analysis of Interatomic Contacts in Proteins.},
	url = {https://academic.oup.com/bioinformatics/article/15/4/327/238299},
