	journal = {Scientifica},
	langid = {english},
	number = {5-6},
	pages = {1--10},
	publisher = {{Hindawi}},
	title = {Comparative {{Metagenomic Analysis}} of {{Electrogenic Microbial Communities}} in {{Differentially Inoculated Swine Wastewater-Fed Microbial Fuel Cells}}},
	url = {https://www.hindawi.com/journals/scientifica/2017/7616359/},
	volume = {2017},
	year = {2017},
