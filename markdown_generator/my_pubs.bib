	pages = {A0094--A0094},
	pmcid = {PMC7953827},
	pmid = {33747696},
	publisher = {{The Mass Spectrometry Society of Japan}},
	title = {Comparison of {{Dimensionality Reduction Methods}} in {{Mass Spectra}} of {{Astrocytoma}} and {{Glioblastoma Tissues}}.},
	url = {https://www.jstage.jst.go.jp/article/massspectrometry/10/1/10_A0094/_article},
	volume = {10},
